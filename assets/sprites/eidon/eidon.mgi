SCRIPT_ANIM_Eidon_Charades::
    SpriteDraw() {
        MoveDraw(4, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(4, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(16, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(4, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(16, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(16, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(4, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(8, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
        MoveDraw(8, 0, 0, SPRITE_Eidon_FaceDown)
        MoveDraw(4, 0, 0, SPRITE_Eidon_Wave)
    }
    Jump(SCRIPT_ANIM_Eidon_Charades)

