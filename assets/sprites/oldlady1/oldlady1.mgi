        Draw(SPRITE_OldLady1_WalkUp1)
        Draw(SPRITE_OldLady1_FaceUp)
        Draw(SPRITE_OldLady1_WalkUp2)
        Draw(SPRITE_OldLady1_FaceUp)
    }
    Jump(SCRIPT_ANIM_OldLady1_WalkUp)

