SCRIPT_ANIM_ShadowMagi6Battle_Victory::
    SpriteDraw() {
        MoveDraw(2, 0, -4, SPRITE_ShadowMagi6Battle_Idle)
        MoveDraw(2, 0, -3, SPRITE_ShadowMagi6Battle_Idle)
        MoveDraw(2, 0, -2, SPRITE_ShadowMagi6Battle_Idle)
        MoveDraw(2, 0, -1, SPRITE_ShadowMagi6Battle_Idle)
        MoveDraw(2, 0, 0, SPRITE_ShadowMagi6Battle_Victory1)
        MoveDraw(2, 0, 1, SPRITE_ShadowMagi6Battle_Victory1)
        MoveDraw(2, 0, 2, SPRITE_ShadowMagi6Battle_Victory1)
        MoveDraw(2, 0, 3, SPRITE_ShadowMagi6Battle_Victory1)
        MoveDraw(2, 0, 4, SPRITE_ShadowMagi6Battle_Victory1)
    }
    .Loop:
        SpriteBlock(8, 0, 0) {
            Draw(SPRITE_ShadowMagi6Battle_Victory1)
            Draw(SPRITE_ShadowMagi6Battle_Victory2)
        }
        Jump(.Loop)

