        Draw(SPRITE_UNK4_FaceUp)
        Draw(SPRITE_UNK4_WalkUp2)
        Draw(SPRITE_UNK4_FaceUp)
    }
    Jump(SCRIPT_ANIM_UNK4_WalkUp)

