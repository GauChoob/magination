SCRIPT_ANIM_PruittRing_MakeRing::
    SpriteDraw() {
        MoveDraw(8, 0, 0, SPRITE_PruittRing_02)
        MoveDraw(8, 0, 0, SPRITE_PruittRing_01)
        MoveDraw(8, 0, 0, SPRITE_PruittRing_02)
        MoveDraw(8, 0, 0, SPRITE_PruittRing_01)
        MoveDraw(16, 0, 0, SPRITE_PruittRing_02)
        MoveDraw(16, 0, 0, SPRITE_PruittRing_03)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_04)
        MoveDraw(8, 0, 0, SPRITE_PruittRing_05)
        MoveDraw(32, 0, 0, SPRITE_PruittRing_06)
        MoveDraw(16, 0, 0, SPRITE_PruittRing_07)
    }
    SetScriptScroll(SCRIPT_ANIM_PruittRing_ScreenShake)
    SpriteDraw() {
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_09)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_09)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_09)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_09)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_09)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_09)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_08)
    }
    SetScriptScroll(SCRIPT_ANIM_PruittRing_ScreenStopShake)
    SpriteDraw() {
        MoveDraw(4, 0, 0, SPRITE_PruittRing_07)
        MoveDraw(4, 0, 0, SPRITE_PruittRing_06)
        MoveDraw(8, 0, 0, SPRITE_PruittRing_04)
    }
