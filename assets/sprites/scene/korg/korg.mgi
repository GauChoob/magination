        MoveDraw(16, 0, 0, SPRITE_KorgBattle_Taunt1)
        MoveDraw(32, 0, 0, SPRITE_KorgBattle_Taunt2)
    }

.Loop::
    SpriteDraw() {
        MoveDraw(4, 0, 0, SPRITE_KorgBattle_Taunt3)
        MoveDraw(4, 0, 0, SPRITE_KorgBattle_Taunt4)
        MoveDraw(16, 0, 0, SPRITE_KorgBattle_Taunt3)
        MoveDraw(16, 0, 0, SPRITE_KorgBattle_Taunt2)
    }
    Jump(.Loop)

