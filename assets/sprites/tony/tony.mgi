SCRIPT_ANIM_Tony_FaceHero::
    Switch(#DIRECTION#) {
        Case(Expr_DIRECTION_UP, .FaceUpOpposite)
        Case(Expr_DIRECTION_DOWN, .FaceDownOpposite)
        Case(Expr_DIRECTION_RIGHT, .FaceRightOpposite)
        Case(Expr_DIRECTION_LEFT, .FaceLeftOpposite)
    }

.FaceUpOpposite::
    ThisSetAnimSingle(SCRIPT_ANIM_Tony_FaceDown)
    WaitEventText(1)
    RestoreActorState()

.FaceDownOpposite::
    ThisSetAnimSingle(SCRIPT_ANIM_Tony_FaceUp)
    WaitEventText(1)
    RestoreActorState()

.FaceLeftOpposite::
    ThisSetAnimSingle(SCRIPT_ANIM_Tony_FaceRight)
    WaitEventText(1)
    RestoreActorState()

.FaceRightOpposite::
    ThisSetAnimSingle(SCRIPT_ANIM_Tony_FaceLeft)
    WaitEventText(1)
    RestoreActorState()