        Draw(SPRITE_UNK2_FaceUp)
        Draw(SPRITE_UNK2_WalkUp2)
        Draw(SPRITE_UNK2_FaceUp)
    }
    Jump(SCRIPT_ANIM_UNK2_WalkUp)

